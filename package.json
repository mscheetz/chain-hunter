{
  "name": "chain-hunter",
  "version": "0.1.0",
  "description": "Chain Hunter allows you to search multiple blockchains for address, contracts, and transactions.",
  "license": "MIT",
  "author": "thechainhunter.com",
  "engines": {
    "node": "10.16.0"
  },
  "scripts": {
    "ng": "ng",
    "config": "node ./scripts/set-env.js",
    "start": "node server.js",
    "build": "npm run config -- --environment=prod && ng build --aot --prod --output-hashing=all --build-optimizer=false",
    "devbuild": "npm run config -- --environment=dev && ng build --output-hashing=all && node server.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pree2e": "webdriver-manager update",
    "preinstall": "npm install -g @angular/cli",
    "postinstall": "ng build --aot --prod",
    "deploy": "git push origin master && git push heroku master"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.10",
    "@amcharts/amcharts4-geodata": "^4.1.19",
    "@angular/animations": "^8.1.3",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.1.3",
    "@angular/compiler": "^8.1.3",
    "@angular/core": "^8.1.3",
    "@angular/forms": "^8.1.3",
    "@angular/platform-browser": "^8.1.3",
    "@angular/platform-browser-dynamic": "^8.1.3",
    "@angular/router": "^8.1.3",
    "axios": "^0.19.2",
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "btc-xpub-address": "^0.2.0",
    "card-validator": "^6.2.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "crypto-ts": "^1.0.2",
    "cryptocurrency-icons": "^0.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-ip": "^1.0.4",
    "font-awesome": "^4.7.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "ngx-cookie-service": "^2.4.0",
    "ngx-qrcode2": "0.0.9",
    "nodemailer": "^6.4.15",
    "pg": "^7.18.2",
    "primeflex": "^1.3.1",
    "primeicons": "^2.0.0",
    "primeng": "^8.1.1",
    "rxjs": "~6.4.0",
    "square-connect": "^2.20200122.0",
    "tslib": "^1.14.1",
    "uuid": "^3.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.29",
    "@angular/cli": "^8.1.3",
    "@angular/compiler-cli": "^8.1.3",
    "@angular/language-service": "^8.1.3",
    "@types/jasmine": "^3.3.16",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "^5.4.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
